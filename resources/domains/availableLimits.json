query availableLimits{
  availableLimits(limit: $lines, generateFile: false, fileType: CSV)
{
  limit
  page
  list {
  accountId
  currentWithdrawalLimit
  globalWithdrawalLimit
  purchaseLimit
  installmentLimit
  internationalCurrentWithdrawalLimit
  internationalGlobalWithdrawalLimit
  internationalPurchaseLimit
  internationalInstallmentLimit
  creditGlobalLimit
  withdrawalAvailableBalance
  purchaseAvailableBalance
  installmentAvailableBalance
  internationalWithdrawalAvailableBalance
  internationalPurchaseAvailableBalance
  totalInstallmentAvailableBalance
  creditGlobalAvailableLimit
  totalInstallmentLimit
  totalInternationalInstallmentLimit
  scoreLimit
  consignedMargin
  maximumLimit
  productId
  accountStatus
  describeAccountStatus
  updatedAt
}
}
}